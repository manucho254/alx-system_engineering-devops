[Unit]
Description=Gunicorn instance to serve airbnb clone
After=network.target
StandardOutput=/tmp/airbnb-error.log
StandardError=/tmp/airbnb-access.log

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
ExecStart=/home/ubuntu/AirBnB_clone_v2/env/bin/gunicorn --workers 3 --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app

[Install]
WantedBy=multi-user.target
